{
    "name": "orchestra/parser",
    "description": "XML Document Parser for Laravel and PHP",
    "keywords": ["laravel", "orchestral", "orchestra-platform", "xml", "parser"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Parser\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0",
        "illuminate/container": "~5.5.0",
        "laravie/parser": "~1.2"
    },
    "require-dev": {
        "illuminate/support": "~5.5.0",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "~6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.5-dev"
        },
        "laravel": {
            "providers": [
                "Orchestra\\Parser\\XmlServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
